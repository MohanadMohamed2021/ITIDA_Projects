#include <iostream>
#include <vector>
#include <algorithm>
#include <math.h>

int main() 
{
    int n,temp,reminder,flag=0;
    std::vector<int>v;
    std::cout<<"enter the number : ";
    std::cin>>n;
    temp=n;
    while(n!=0)
    {
        reminder=n%10;
        v.push_back(reminder);
        n/=10;
    }
    for(int i=0;i<v.size();i++)
    {
        for(int j= i+1 ; j<v.size() ; j++)
        {
            if(v[i]==v[j])
            {
                flag=1;
            }
        }
        if(flag==1)
        {
            std::cout<<temp<<" is not unique number"<<std::endl;
            break;
        }
    }
    if(flag==0)
    {
        std::cout<<temp<<" is unique number"<<std::endl;
    }
    return 0;
}

